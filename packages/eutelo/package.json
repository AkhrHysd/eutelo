{
  "name": "@eutelo/eutelo",
  "version": "0.3.0",
  "description": "Eutelo エコシステムの全パッケージをまとめてインストールするメタパッケージ",
  "license": "MIT",
  "type": "module",
  "main": "./index.js",
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/eutelo/eutelo.git",
    "directory": "packages/eutelo"
  },
  "keywords": [
    "eutelo",
    "documentation",
    "cli",
    "lint",
    "scaffold",
    "guard"
  ],
  "dependencies": {
    "@eutelo/core": "file:../core",
    "@eutelo/infrastructure": "file:../infrastructure",
    "@eutelo/distribution": "file:../distribution",
    "@eutelo/cli": "file:../cli",
    "@eutelo/biome-doc-lint": "file:../biome-doc-lint",
    "@eutelo/commander": "file:../commander",
    "eslint-plugin-eutelo-docs": "file:../eslint-plugin-docs"
  },
  "peerDependencies": {
    "@biomejs/biome": ">=1.9.0",
    "eslint": ">=8"
  },
  "peerDependenciesMeta": {
    "@biomejs/biome": {
      "optional": true
    },
    "eslint": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
